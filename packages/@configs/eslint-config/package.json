{
  "name": "@andreidmt/eslint-config",
  "version": "1.0.0",
  "description": "Andrei Dumitrescu's ESLint config",
  "license": "MIT",
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidmt",
    "email": "andrei.dumitrescu@pm.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreidmt/mono.git",
    "directory": "packages/@configs/eslint-config"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/andreidmt",
    "access": "public"
  },
  "scripts": {
    "----UTIL": "",
    "format": "eslint --fix .",
    "----CODE-ANALYSIS": "",
    "lint.js": "eslint --quiet .",
    "lint.md": "markdownlint '*.md' --ignore CHANGELOG.md",
